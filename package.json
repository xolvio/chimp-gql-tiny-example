{
  "name": "chimp-gql-tiny-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "graphql:codegen": "chimp gql:generate",
    "graphql:generateAll": "npm run graphql:codegen && npm run graphql:generateSchema && npm run graphql:typegen && npm run prettify:all",
    "graphql:generateSchema": "ts-node -r tsconfig-paths/register ./generated/graphql/printSchema.ts > ./schema.graphql",
    "graphql:typegen": "graphql-codegen --config codegen.js && node ./fix-generated",
    "postinstall": "npm run graphql:generateAll",
    "prettify:all": "npx prettier --write \"src/**/*.ts\" \"generated/**/*.ts\" --loglevel error",
    "test": "jest",
    "start": "ts-node -r tsconfig-paths/register ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xolvio/chimp-gql-tiny-example.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xolvio/chimp-gql-tiny-example/issues"
  },
  "homepage": "https://github.com/xolvio/chimp-gql-tiny-example#readme",
  "dependencies": {
    "@apollo/federation": "0.17.0",
    "@graphql-tools/graphql-file-loader": "^6.0.18",
    "@graphql-tools/load": "^6.0.18",
    "@graphql-tools/merge": "6.0.14",
    "@graphql-tools/schema": "^6.0.18",
    "apollo-datasource-rest": "^0.9.3",
    "apollo-server": "2.16.1",
    "graphql": "15.3.0",
    "shelljs": "0.8.4",
    "tsconfig-paths": "3.9.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "1.17.0",
    "@graphql-codegen/cli": "1.17.0",
    "@graphql-codegen/typescript": "1.17.0",
    "@graphql-codegen/typescript-mongodb": "1.17.0",
    "@graphql-codegen/typescript-operations": "1.17.0",
    "@graphql-codegen/typescript-resolvers": "1.17.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link-schema": "1.2.5",
    "chimp": "2.4.0",
    "graphql-import": "1.0.2",
    "chimp-graphql-codegen-plugin": "0.1.0",
    "jest": "26.4.0",
    "prettier": "2.0.5",
    "testdouble": "^3.16.1",
    "ts-jest": "26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "3.9.7"
  }
}
